  "version": "0.2.0",
  "configurations": [
    {
      "name": "Django: MakeMigration",
      "type": "debugpy",
      "request": "launch",
      "args": ["makemigrations"],
      "django": true,
      "autoStartBrowser": false,
      "program": "${workspaceFolder}/backend/manage.py"
    },
    {
      "name": "Django: runserver",
      "type": "debugpy",
      "request": "launch",
      "args": ["runserver", "192.168.1.158:8000"],
      "django": true,
      "autoStartBrowser": false,
      "program": "${workspaceFolder}/backend/manage.py"
    },
    {
      "name": "Django: migrate",
      "type": "debugpy",
      "request": "launch",
      "args": ["migrate"],
      "django": true,
      "autoStartBrowser": false,
      "program": "${workspaceFolder}/backend/manage.py"
    }
  ]
}
